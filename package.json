{
  "name": "rollbar-php-wordpress",
  "version": "2.6.4",
  "description": "Official Rollbar full-stack error tracking for WordPress supported by Rollbar, Inc.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tests-composer-update": "wp-env run tests-cli --env-cwd=wp-content/plugins/rollbar-php-wordpress composer update",
    "tests-composer-install": "wp-env run tests-cli --env-cwd=wp-content/plugins/rollbar-php-wordpress composer install --no-interaction",
    "pretest:php": "npm-run-all tests-composer-update tests-composer-install ",
    "test:php": "wp-env run tests-wordpress /var/www/html/wp-content/plugins/rollbar-php-wordpress/vendor/bin/phpunit -c /var/www/html/wp-content/plugins/rollbar-php-wordpress/phpunit.xml.dist",
    "start": "wp-env start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollbar/rollbar-php-wordpress.git"
  },
  "author": "Rollbar, Inc.",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/rollbar/rollbar-php-wordpress/issues"
  },
  "homepage": "https://github.com/rollbar/rollbar-php-wordpress#readme",
  "dependencies": {
    "@wordpress/env": "^8.3.0",
    "npm-run-all": "^4.1.5"
  }
}
